import math
import matplotlib.pyplot as plt

if __name__ =="__main__":
    x = [float(i)/100 for i in range(1,300)]
    y = [math.log(i) for i in x]
    plt.plot(x,y,'r-',linewidth=2,label='logCurve')
    plt.legend(loc='upper left')
    plt.grid(True)
    plt.show()
    
